@page
@model QueueUploadModel
@{
    ViewData["Title"] = "Service Bus Queue Upload";
}

<h1>@ViewData["Title"]</h1>

<form method="post" enctype="multipart/form-data">
    <div>
        <p>Select a text file to be posted to an Azure Queue:</p>
        <input type="file" name="uploadedFile" />
        <input type="submit" value="Upload" />
    </div>
</form>